{
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "commonjs": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react", "prettier", "react-hooks", "import"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "plugin:react/recommended", "google", "prettier", "prettier/react"
  ],
  "rules": {
    "require-jsdoc": 0,
    "prettier/prettier": "error",
    "no-invalid-this": 0,
    "react/prop-types": 0,
    "import/order": ["error",
      {
        "newlines-between": "always",
        "alphabetize": {"order": "asc", "caseInsensitive": true}
      }
    ]
  }
}
